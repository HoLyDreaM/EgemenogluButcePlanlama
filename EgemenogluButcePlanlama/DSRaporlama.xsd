              <DbSource ConnectionRef="Cs (Settings)" DbObjectName="MikroDB_V12_2011.dbo.ODEME_EMIRLERI" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Fill" GenerateShortCommands="false" GeneratorSourceName="Fill" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT dbo.fn_CekSenetKisa(sck_tip) AS 'TİPİ', CASE SUBSTRING(sck_sahip_cari_kodu, (CASE LEFT(sck_sahip_cari_kodu, 3) 
                         WHEN '950' THEN 12 ELSE 8 END), 2) 
                         WHEN '01' THEN 'İSTANBUL' WHEN '02' THEN 'BURSA' WHEN '03' THEN 'İZMİR' WHEN '04' THEN 'ADANA' WHEN '05' THEN 'ANKARA' ELSE 'TANIMSIZ' END AS İL,
                          sck_srmmrk AS 'SM Kodu', dbo.fn_SorumlulukMerkeziIsmi(sck_srmmrk) AS 'SM Adi', dbo.fn_CekSenetPos(sck_sonpoz, 0) AS 'SON POZİSYON', 
                         sck_refno AS 'REFERANS', sck_sahip_cari_kodu AS 'C/H Kod', dbo.fn_CarininIsminiBul(sck_sahip_cari_cins, sck_sahip_cari_kodu) AS 'C/H Adı', 
                         sck_nerede_cari_kodu AS 'Ilk Pozisyon C/H Kod', dbo.fn_CarininIsminiBul(sck_nerede_cari_cins, sck_nerede_cari_kodu) AS 'lk Pozisyon C/H Adı', 
                         sck_no AS 'ÇEK NO', sck_vade AS 'VADE TARİHİ', sck_tutar AS 'TUTAR',sck_tutar AS 'ORJ_TUTAR', sck_odenen AS 'ÖDENEN', dbo.fn_DovizIsmi(sck_doviz) AS 'DÖVİZ', 
                         sck_doviz_kur AS 'DÖVİZ KURU', sck_tutar - sck_odenen AS 'KALAN MİKTAR', dbo.fn_gunfarkibul(sck_vade, GETDATE()) AS 'GÜN'
FROM            ODEME_EMIRLERI WITH (NOLOCK)
WHERE        (sck_vade BETWEEN CONVERT(DATETIME, '2011-01-01', 102) AND CONVERT(DATETIME, '2020-10-10', 102))
ORDER BY 'ÇEK NO'</CommandText>
