                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 'Masraf' as tip, cha_belge_tarih, cha_kod,  cha_meblag, cha_aciklama, cha_evrakno_seri, cha_evrakno_sira, cha_belge_no,cha_srmrkkodu,dbo.fn_CarininIsminiBul(0,cha_kod) AS cariAdi,  dbo.fn_SorumlulukMerkeziAdiBul(cha_srmrkkodu)as smAdi,  cha_kasa_hizkod,dbo.fn_MasrafIsmi(cha_kod) masrafAdi,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-01-01', 23) AND CONVERT(VARCHAR, '2013-01-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS Toplam,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-01-01', 23) AND CONVERT(VARCHAR, '2013-01-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS Ocak,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-02-01', 23) AND CONVERT(VARCHAR, '2013-02-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS subat,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-03-01', 23) AND CONVERT(VARCHAR, '2013-03-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS mart,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-04-01', 23) AND CONVERT(VARCHAR, '2013-04-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS nisan,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-05-01', 23) AND CONVERT(VARCHAR, '2013-05-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS mayis,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-06-01', 23) AND CONVERT(VARCHAR, '2013-06-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS haziran,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-07-01', 23) AND CONVERT(VARCHAR, '2013-07-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS temmuz,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-08-01', 23) AND CONVERT(VARCHAR, '2013-08-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS agustos,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-09-01', 23) AND CONVERT(VARCHAR, '2013-09-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS eylul,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-10-01', 23) AND CONVERT(VARCHAR, '2013-10-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS ekim,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-11-01', 23) AND CONVERT(VARCHAR, '2013-11-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS kasim,  CASE WHEN  cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-12-01', 23) AND CONVERT(VARCHAR, '2013-12-31', 23) THEN isnull(SUM(cha_meblag),0) else 0 END AS aralik, 	CASE left(cha_srmrkkodu,2)  WHEN '01' THEN 'ISTANBUL'  WHEN '02' THEN 'BURSA'  WHEN '03' THEN 'IZMIR'  WHEN '04' THEN 'ADANA'  WHEN '05' THEN 'ANKARA'  when '00' then 'Ortak Cari Islemler'  when '99' then 'Yunus EGEMENOGLU'  ELSE 'Tanımsız' END AS IL         from CARI_HESAP_HAREKETLERI  WHERE  cha_kod IN (SELECT his_kod FROM MASRAF_HESAPLARI  WHERE left(his_muhkod,3)&lt;&gt;'840')  and (cha_belge_tarih BETWEEN CONVERT(VARCHAR, '2013-01-01', 23) AND CONVERT(VARCHAR, '2013-12-31', 23))  GROUP BY cha_belge_tarih, cha_kod, cha_kasa_hizkod,  cha_meblag, cha_aciklama, cha_srmrkkodu,cha_evrakno_seri, cha_evrakno_sira, cha_belge_no </CommandText>
