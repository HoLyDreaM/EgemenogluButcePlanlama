          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="LikiditeRaporuTableAdapter" GeneratorDataComponentClassName="LikiditeRaporuTableAdapter" Name="LikiditeRaporu" UserDataComponentName="LikiditeRaporuTableAdapter">
